<%include ./partials/header.ejs%>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-10 text-white">
        <div class="cards flex flex-wrap gap-10">
            <% user.posts.forEach(function(elem){ %>
                <div class="card w-32">
                    <div class="w-full h-40 bg-sky-300 rounded-xl overflow-hidden">
                        <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                    </div>
                    <h3 class="text-sm mt-2">
                        <%= elem.title %>
                    </h3>
                </div>
                <%})%>

        </div>
    </div>
    <%include ./partials/footer.ejs%>